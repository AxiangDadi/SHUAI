        })
        $(function () {
            $("#quanxuan").click(function () {
                var xz = document.getElementById("quanxuan");
                var x = xz.checked;
                if (x == false) {
                    $("input[type='checkbox']").attr("checked",false);
                }
                if(x==true)  {
                    $("input[type='checkbox']").prop("checked",true);
                }
            })
        })


